Rem Attribute VBA_ModuleType=VBAModule
Option VBASupport 1
Function RiesgoECV(sexo As String, edad As Integer, fumador As String, PAS As Double, colesterol As Double) As Double

'CALCULO DE RIESGO 0% FEMENINO
'condicion 1
If sexo = "F" Then
    If (edad >= 40 And edad <= 49) Then
        If (fumador = "NO") Then
                If (PAS > 0) And (PAS < 140) Then
                    If (colesterol > 0) And (colesterol < 151) Then
                        RiesgoECV = 0
                    End If
                End If
        End If
    End If
End If

'----------------------------------------------------------
'CALCULO DE RIESGO 0% FEMENINO
'condicion 2
