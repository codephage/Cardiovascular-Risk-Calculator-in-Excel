Function RiskCV(sex As String, age As Integer, smoke As String, PAS As Double, cholesterol As Double) As Double

'RISK CALCULATION 0% FEMENINO
'condition 1
If sex = "F" Then
    If (age >= 40 And age <= 49) Then
        If (smoke = "NO") Then
                If (PAS > 0) And (PAS < 140) Then
                    If (cholesterol > 0) And (cholesterol < 151) Then
                        RiskCV = 0
                    End If
                End If
        End If
    End If
End If

'----------------------------------------------------------
'RISK CALCULATION 0% FEMENINO
'condition 2
